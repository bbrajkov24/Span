@model IEnumerable<Span.Models.Podaci>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>List</title>
</head>

<body>
    <table>
        <thead>
        </thead>
        <tfoot>
        </tfoot>
        <tbody>
            @foreach (var podatak in Model)
            {
                <tr>
                    <td>@podatak.PodaciID</td>
                    <td>@podatak.Ime</td>
                    <td>@podatak.Prezime</td>
                    <td>@podatak.PBr</td>
                    <td>@podatak.Grad</td>
                    <td>@podatak.Telefon</td>
                </tr>
            }
        </tbody>
    </table>


</body>
</html>